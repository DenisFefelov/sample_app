<% provide(:title, 'Sign up') %>
<h1>Sign up</h1>

<div class="row">
  <div class="user_info_container">
   <%= form_for(@user) do |f| %>
      <%= render 'shared/error_messages', object: f.object %>
      <%= render 'fields', f: f %>
      <%= f.submit "Create my account", class: "primary_button" %>
    <% end %>
  </div>
</div>